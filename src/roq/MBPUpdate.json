{
  "name": "roq/MBPUpdate",
  "type": "struct",
  "comment": "Represents the update status of a single aggregate price level in the order book",
  "values": [
    {
      "name": "price",
      "type": "std/double",
      "comment": "Price"
    },
    {
      "name": "quantity",
      "type": "std/double",
      "default": " = {}",
      "comment": "Total quantity available at price (zero means remove price level)"
    },
    {
      "name": "implied_quantity",
      "type": "std/double",
      "default": " = NaN",
      "comment": "Total implied quantity at price (optional)"
    },
    {
      "name": "number_of_orders",
      "type": "std/uint16",
      "default": " = {}",
      "comment": "Number of orders at price (optional)"
    },
    {
      "name": "update_action",
      "type": "roq/UpdateAction",
      "comment": "Type of update action"
    },
    {
      "name": "price_level",
      "type": "std/uint32",
      "default": " = {}",
      "comment": "Price level (0-based indexing)"
    }
  ]
}
