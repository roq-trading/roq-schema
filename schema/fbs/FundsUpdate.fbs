namespace roq.fbs;

table FundsUpdate {
  stream_id:uint16;
  account:string;
  currency:string;
  balance:double = nan;
  hold:double = nan;
  external_account:string;
  update_type:UpdateType;
  exchange_time_utc:int64;
  sending_time_utc:int64;
  margin_mode:MarginMode;
  exchange_sequence:uint64;
}
